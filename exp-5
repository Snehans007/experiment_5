medium:

#include<stdio.h>
#include<string.h>
void main()
{
    char str1[100]="cprograming";
    int i=0,j=strlen(str1)-1;
    while(i<j){
        int t=str1[i];
        str1[i]=str1[j];
        str1[j]=t;
        i++;
        j--;
        
    }
    printf("%s",str1);
    
}

==================================================================================================================================================================================================================
high:



#include<stdio.h>
#include<string.h>
void main()
{
    char str[100];
    int weight=0;
    printf(" enter a string");
    scanf("%s",str);

    
    
    for(int i=1; str[i]!='\0' ;i++){
    weight += (str[i] -'a'+1);
    
}
printf(" total number of weight:%d",weight);
}

